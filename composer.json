{
    "name": "particleacademy/laravel-fun-lab",
    "description": "Analytics-driven gamification layer for Laravel applications. Track user engagement through awards, achievements, and prizes.",
    "type": "library",
    "keywords": [
        "laravel",
        "gamification",
        "analytics",
        "engagement",
        "achievements",
        "awards",
        "prizes",
        "leaderboards"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Particle Academy",
            "email": "dev@particleacademy.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/events": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelFunLab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFunLab\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFunLab\\LFLServiceProvider"
            ],
            "aliases": {
                "LFL": "LaravelFunLab\\Facades\\LFL"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
